[ids]

*

[main]

capslock = layer(control)
control = esc
esc = capslock
rightshift = muhenkan
rightcontrol = katakanahiragana

rightalt = layer(nav)

q = q
w = w
e = f
r = p
t = b

y = j
u = l
i = u
o = y
p = ;

a = a
s = r
d = s
f = t
g = g

h = m
j = n
k = e
l = i
; = o

z = x
x = c
c = d
v = v
b = z

n = k
m = h

[nav]
- = volumedown
= = volumeup
0 = playpause
9 = mute
' = backspace
[ = delete

j = left
k = down
l = up
; = right

u = h
i = j
o = k
p = l

m = home
, = pagedown
. = pageup
/ = end

a = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- -20 0")
s = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 0 20")
d = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 0 -20")
f = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 20 0")

q = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- -200 0")
w = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 0 200")
e = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 0 -200")
r = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 200 0")

z = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- -1000 0")
x = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 0 1000")
c = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 0 -1000")
v = command(DISPLAY=:0 su -c - peter "/usr/bin/xdotool mousemove_relative -- 1000 0")

g = leftmouse
h = rightmouse
